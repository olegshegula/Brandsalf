<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="LoginTest">
	<test verbose="2" name="testLoginOK">
		<classes>
			<class name="com.bs.selenium.functional.LoginTest">
				<methods>
					<include name="testLoginOK" />
				</methods>
			</class>
		</classes>
	</test>
	<test verbose="2" name="testLogOut">
		<classes>
			<class name="com.bs.selenium.functional.LoginTest">
				<methods>
					<include name="testLogOut" />
				</methods>
			</class>
		</classes>
	</test> 	
	<test verbose="2" name="testLoginFailedBlankLoginPasswd">
		<classes>
			<class name="com.bs.selenium.functional.LoginTest">
				<methods>
					<include name="testLoginFailedBlankLoginPasswd" />
				</methods>
			</class>
		</classes>		
	</test> 	
	<test verbose="2" name="testLoginFailedWrongPasswd">
		<classes>
			<class name="com.bs.selenium.functional.LoginTest">
				<methods>
					<include name="testLoginFailedWrongPasswd" />
				</methods>
			</class>
		</classes>		
	</test>
	<test verbose="2" name="testLoginFailedWrongLogin">
		<classes>
			<class name="com.bs.selenium.functional.LoginTest">
				<methods>
					<include name="testLoginFailedWrongLogin" />
				</methods>
			</class>
		</classes>		
	</test> 	 	 	 		
</suite>
<suite name="EditProfileTest">
<test verbose="2" name="testEnterAllFieldsCorrect">
		<classes>
			<class name="com.bs.selenium.functional.EditProfileTest">
				<methods>
					<include name="testEnterAllFieldsCorrect" />
				</methods>
			</class>
		</classes>		
	</test> 	 	
</suite>suite>